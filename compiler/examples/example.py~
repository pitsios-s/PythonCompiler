def add(x,y):
	return x + y
def fib(n):# write Fibonacci series up to n
		a = 0
		b = [1,2]
		for a in b:
			 print a
			 a = b
			 b = a + b
def ifcmpl():
	if add(1,2)<add(1,2)+1:
		print add(1,2)+1, " is grater"

def findmax(x,y):
	if x > y:
		return x
	if y > x:
		return y

def funcwithdef(name,university="aueb"):
	print name, " studies in ", university

	
print add(add(1,1),3) #prints 5
w = fib(5)
print w+findmax(1,2)
